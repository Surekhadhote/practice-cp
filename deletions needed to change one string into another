#include<bits/stdc++.h>
using namespace std;

int main(){
int n;
string s,t;
cin>>n>>s>>t;
int c = 0;

for(int i =0;i<n;i++){
if(s == t)
{
cout<<c<<endl;
break;
}
    s.erase(0, 1);
    t.pop_back();
 c++;
}
    return 0;
}

//ques: https://www.hackerearth.com/practice/algorithms/string-algorithm/basics-of-string-manipulation/practice-problems/algorithm/rotation-1-38ecf5a7/
